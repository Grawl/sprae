{
  "name": "sprae",
  "description": "Reactive directives with expressions for DOM microtemplating.",
  "version": "1.0.0",
  "main": "sprae.js",
  "type": "module",
  "dependencies": {
    "@preact/signals-core": "^1.2.2",
    "element-props": "^2.3.0",
    "signal-struct": "^1.0.0",
    "sube": "^2.2.1"
  },
  "devDependencies": {
    "@preact/signals": "^1.1.2",
    "browser-env": "^3.3.0",
    "es-module-shims": "^1.6.2",
    "esbuild": "^0.15.14",
    "hyperf": "^1.4.0",
    "terser": "^5.15.1",
    "tst": "^7.1.1",
    "value-ref": "^2.1.0",
    "wait-please": "^3.1.0"
  },
  "scripts": {
    "test": "node -r browser-env/register test/test.js",
